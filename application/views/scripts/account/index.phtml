		gFields = {
			"home" : "Home Address",
			"ship" : "Shipping Address",
			"bill" : "Billing Address",
			"work" : "Work Information",
			"other" : "Other Information"
		};

		
		

		function showAddress(address_type, obj) {
			for ( var field in gFields ) {
				addEl = document.getElementById(field + 'Header');
				if (addEl) {
					addEl.className = 'tab';
					addEl.innerHTML = '<a href="#" onclick="javascript:return showAddress(\'' + field + '\');">' + gFields[field] + '</a>';
				}
				Hide(field + 'Panel');
			}

			addEl = document.getElementById(address_type + 'Header');
			addEl.className = 'tab_live';
			addEl.innerHTML = gFields[address_type];
			Show(address_type + 'Panel');
			return false;
		}

		document.onkeydown = function (evt) {
			var keyCode = evt ? (evt.which ? evt.which : evt.keyCode) : event.keyCode;
			var eventTarget = evt ? evt.target : event.srcElement;
			var textField = eventTarget.type == 'text' || eventTarget.type == 'password' || eventTarget.type == 'checkbox';
			if (keyCode == 13 && textField && eventTarget.form.name) {
				switch(eventTarget.form.name) {
					case "changepassform":
						document.forms[eventTarget.form.name].submit();
					case "addressform":
						document.forms[eventTarget.form.name].submit();
					default: break;
				}
			}
		};

		
		

		function unload() {
			if (gChanged)
				return "You have unsaved changes."
		}


